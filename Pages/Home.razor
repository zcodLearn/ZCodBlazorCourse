@page "/"
@page "/Home"
@page "/Index"
@page "/Account/{IndexID}/{IndexCode:int}"
@inject NavigationManager nav

<PageTitle>Home</PageTitle>

<h1>Hello, world!</h1>

<h5>
   Index Code : @IndexCode
</h5>
<h5>
   Index ID : @IndexID
</h5>
<button class="btn btn-primary" @onclick="NavigateToCounter">
    Navigate
</button>
@code {
    [Parameter]
    public string? IndexID { get; set; }
    [Parameter]
    public int? IndexCode { get; set; }

    public void NavigateToCounter()
    {
        nav.NavigateTo("/counter" , false);
    }
}
